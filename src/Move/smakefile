.c.o:
	sc $(CFLAGS) $(SCOPTIONS) $(*).c

CFLAGS = STRIPDEBUG NODEBUG LIB sc:lib/sc.lib LIB lib:small.lib BATCH NOICONS 
LDFLAGS = LIB sc:lib/sc.lib LIB lib:small.lib NOICONS 
SCOPTIONS = DATA=NEAR CODE=NEAR NOSTACKCHECK NOICONS COMMENTNEST STRUCTUREEQUIVALENCE OPTIMIZE MAP NOVERSION UTILITYLIBRARY INCLUDEDIR=include: 

OBJS= move.o list.o

move: $(OBJS)
	slink $(LDFLAGS) from sc:lib/cres.o $(OBJS) to move

# Clean target
clean:
	-delete \#?.(lib|o|do|nbo|dnbo|gst|lst|map) move

# Install target
install: move
	copy move //SDK/C/Move CLONE