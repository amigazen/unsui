CFLAGS= -mi -mRR -proto -ms -r -E DCC_ERRORS
CC=     dcc

SRC=exec.c getpubscreen.c getvolume.c match.c setrc.c nop.c tee.c
FILES=$(SRC:*.c:*)

all:    $(FILES) lines

$(FILES): $(SRC)
    $(CC) $(CFLAGS) %(right) -o /bin/%(left)

lines:  lines.c
    $(CC) $(CFLAGS) %(right) -o /bin/%(left) -l amiga30s.lib

