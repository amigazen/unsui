#
# smakefile for md5sum (Amiga [SAS/C v6.x])
#
# created by Peter Simons <simons@peti.GUN.de>
#

CFLAGS  = PARAMETERS=REGISTERS STRINGMERGE \
          NOSTACKCHECK OPTIMIZE OPTIMIZERSIZE NOOPTIMIZERINLINE \
          OPTIMIZERCOMPLEXITY=5 OPTIMIZERDEPTH=5 OPTIMIZERRECURDEPTH=5
LFLAGS  = STARTUP=cres

CC      = sc

PROJ    = md5sum
OBJS    = getopt.o md5.o md5sum.o

.c.o:
        $(CC) $(CFLAGS) $*.c

$(PROJ):        $(OBJS) $(LIBS)
        $(CC) $(OBJS) TO $(PROJ) LINK $(LFLAGS)
        delete $(PROJ).lnk QUIET

## Dependencies ##
getopt.o : getopt.c getopt.h
md5.o : md5.c md5.h
md5sum.o : md5sum.c md5.h getopt.h

