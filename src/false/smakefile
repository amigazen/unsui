# false command SMakefile for unsui POSIX runtime

# Tools
CC = sc
LD = slink
RM = delete
CFLAGS = STRIPDEBUG NODEBUG LIB sc:lib/sc.lib LIB lib:small.lib BATCH NOICONS
LDFLAGS = LIB sc:lib/sc.lib LIB lib:small.lib NOICONS
SCOPTIONS = DATA=NEAR CODE=NEAR NOSTACKCHECK NOICONS COMMENTNEST STRUCTUREEQUIVALENCE OPTIMIZE MAP NOVERSION UTILITYLIBRARY INCLUDEDIR=include: INCLUDEDIR=/common

# Object files for false
OBJS = sc:lib/cres.o false.o

# Main target
all: false

# Build false command
false: $(OBJS)
	$(LD) $(LDFLAGS) FROM $(OBJS) TO false

# Compile source files
.c.o:
	$(CC) $(SCOPTIONS) $< TO $@

# Header dependencies
false.o: false.c

# Clean target
clean:
	-$(RM) \#?.(lib|o|do|nbo|dnbo|gst|lst|map) false

# Install target
install: false
	copy false //SDK/C/false
